<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.user.MemberMergerHis" table="MEMBER_MERGER_HIS"  lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="memberId" type="long" column="MEMBERID"/>
		<property name="memberPhone" type="string" column="MEMBER_PHONE"/>
		<property name="mergerMemberId" type="long" column="MERGER_MEMBERID"/>
		<property name="mergerMemberNickName" type="string" column="MERGER_MEMBER_NICKNAME"/>
		<property name="operationUserId" type="long" column="OPERATION_USERID"/>
		<property name="addtime" type="timestamp" column="ADDTIME"/>
		<property name="updatetime" type="timestamp" column="UPDATETIME"/>
		<property name="otherInfo" type="string" column="OTHERINFO"/>
		<property name="status" type="string" column="STATUS"/>
		<property name="remark" type="string" column="REMARK"/>
		<property name="bindStep" type="long" column="BIND_STEP"/>
	</class>
</hibernate-mapping>
