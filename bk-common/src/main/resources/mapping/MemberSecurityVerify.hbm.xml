<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	 "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	 	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.user.MemberSecurityVerify" table="MEMBER_SECURITY_VERIFY" dynamic-update="true" lazy="false">
		<id name="securityCode" type="string" column="SECURITYCODE">
			<generator class="assigned" />
		</id>
		<property name="memberid" type="long" column="MEMBERID"/>
		<property name="deviceId" type="string" column="DEVICEID"/>
		<property name="addtime" type="timestamp" column="ADDTIME"/>
	</class>
</hibernate-mapping>

